#include <iostream>

using namespace std;

int main(){
    int nilai[5];
    int a=0;
    int i;
    int dea=0;

     for(i=0;i<5;i++){
        a++;
        cout<<"Masukan Data ke-"<<a<<"  = ";
        cin>>nilai[i];
    }
     for (i=0; i<5; i++){


    if (nilai[i]>dea)
        dea=nilai[i];

    }
    cout<<"Nilai paling besar = "<<dea<<endl;

    return 0;
}
